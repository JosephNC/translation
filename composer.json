{
    "name": "josephnc/translation",
    "description": "An automatic Laravel 5 translator",
    "type": "library",
    "keywords": [
        "internationalization",
        "localization",
        "translation",
        "translator",
        "translate",
        "localize",
        "laravel",
        "lang"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "JosephNC",
            "email": "chukwudinwakpaka@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "illuminate/database": "5.*",
        "illuminate/support": "5.*",
        "stichoza/google-translate-php": "^4.0",
        "viniciusgava/google-translate-api": "^2.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "~4|~5"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "JosephNC\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JosephNC\\Translation\\Tests\\": "tests/"
        }
    }
}
